{
  "name": "@workpop/analytics-middleware",
  "version": "0.0.1",
  "description": "Redux middleware for Workpop Analytics",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha tests/index.js",
    "lint": "eslint .",
    "prepublish": "babel ./src --ignore test --out-dir ./dist"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "6.21.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "6.2.10",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.1.2",
    "redux": "^3.5.2",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "babel-runtime": "6.x.x"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "max-len": 0,
      "prefer-arrow-callback": 0,
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  },
  "keywords": [
    "Analytics"
  ],
  "author": "Workpop",
  "license": "MIT"
}
